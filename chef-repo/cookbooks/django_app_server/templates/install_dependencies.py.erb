import subprocess

path = '<%= @dep_file_path %>'
with open(path) as system_dependencies:
  deps = ' '.join([line.rstrip() for line in system_dependencies])

# Need to redirect output from cmd for Chef to execute correctly for some reason
cmd = 'apt-get install -y ' + deps + ' > /dev/null'

subprocess.Popen(cmd, shell=True)
